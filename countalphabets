#include <iostream>
#include <cstring>
using namespace std;

int main(int argc, char* argv[]) {
    int freq[26] = {0};

    if (argc < 2) {
        cout << "No input provided.\n";
        return 1;
    }

    for (int i = 1; i < argc; i++) {
        for (int j = 0; argv[i][j] != '\0'; j++) {
            char ch = tolower(argv[i][j]);
            if (ch >= 'a' && ch <= 'z') freq[ch - 'a']++;
        }
    }

    cout << "Alphabet Frequency:\n";
    for (int i = 0; i < 26; i++) {
        if (freq[i] > 0) cout << char(i + 'a') << " : " << freq[i] << endl;
    }

    return 0;
}
